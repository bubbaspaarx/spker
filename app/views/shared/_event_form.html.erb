<%= simple_form_for([user, event]) do |f| %>
  <%= f.input :name%>
  <%= f.input :address%>
  <%= f.input :postcode%>
  <%= f.input :date, as: :string, input_html: { class: 'datepicker' } %>
  <%= f.input :start_time, as: :string, input_html: { class: 'timepicker' } %>
  <%= f.input :end_time, as: :string, input_html: { class: 'timepicker' } %>

<% end %>
